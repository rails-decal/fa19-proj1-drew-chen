<h3>Trainer</h3>
<p>Name: <%= @trainer.name if @trainer.name %></p>
<p>Email: <%= @trainer.email if @trainer.email %></p>
<% for pokemon in @trainerPokemons%>
	<% if pokemon and current_trainer %>
  		<% if pokemon.ndex %>
   			<%= image_tag("pokemon/#{pokemon.ndex}.png", alt: "pokemon") %>
			<br>Name: <%= pokemon.name %><br> Level: <%= pokemon.level %><br>
		<% end %>
  <% end %>
<% end %>